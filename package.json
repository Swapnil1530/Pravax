{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "setup": "bash tools/scripts/setup-dev.sh",
    "infra:up": "docker compose up -d || docker-compose up -d",
    "infra:down": "docker compose down || docker-compose down",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@nestjs/jwt": "^11.0.0"
  }
}